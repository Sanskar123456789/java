package simplilearn.array.tr;

public class cal {
	public int num1;
	public int num2;
	public int result= 0;
	
	public void add(int a,int b) {
		this.num1 = a;
		this.num2 = b;
		this.result = a+b;
	}
	
	public void sub(int a,int b) {
		this.num1 = a;
		this.num2 = b;
		this.result = a-b;
	}
	
	public void mul(int a,int b) {
		this.num1 = a;
		this.num2 = b;
		this.result = a*b;
	}
	
	public void div(int a,int b) {
		this.num1 = a;
		this.num2 = b;
		this.result = a/b;
	}
}
